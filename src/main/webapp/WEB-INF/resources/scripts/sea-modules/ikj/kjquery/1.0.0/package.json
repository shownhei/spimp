{
  "name": "kjquery",
  "family": "ikj",
  "version": "1.0.0",
  "description": "将所有jquery插件进行打包",
  "keywords": ["widget"],
  "author": "Stone Lee <istonelee@gmail.com> (http://stonelee.info)",
  "repository": {
    "type": "git",
    "url": "http://10.10.22.18/stonelee/ikj-kjquery.git"
  },
  "bugs": {
    "url": "http://10.10.22.18/stonelee/ikj-kjquery/issues"
  },
  "spm": {
    "alias": {
      "$": "$",
      "jquery-migrate": "ikj/jquery-migrate/1.2.1/jquery-migrate",
      "jquery-ui": "ikj/jquery-ui/1.10.3/jquery-ui",
      "ztree": "ikj/ztree/3.5.14/ztree",
      "rest": "ikj/rest/1.0.0/rest",
      "gritter": "ikj/gritter/1.0.0/gritter",
      "serialize-object": "ikj/serialize-object/1.0.0/serialize-object",
      "bootstrap": "ikj/bootstrap/2.3.2/bootstrap",
      "autocomplete": "ikj/autocomplete/1.2.3/autocomplete",
      "datepicker": "ikj/datepicker/1.2.0/datepicker",
      "datetimepicker2": "ikj/datetimepicker2/3.0.0/datetimepicker",
      "cookie": "ikj/cookie/1.4.0/cookie",
      "xheditor": "ikj/xheditor/1.2.1/xheditor",
      "select2": "jquery/select2/3.4.5/select2-zh-cn"
    },
    "output": ["kjquery.js"]
  }
}
